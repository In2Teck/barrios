<div style="margin: 1em">
  <p>
    Download:
    <%= link_to "CSV", csv_path(format: "csv") %> |
    <%= link_to "Excel", csv_path(format: "xls") %>
  </p>
  <br/>
  <table>
    <tr>
      <th>ID</th>
      <th>Facebook ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
    </tr>
    <% @users.each do |user|%>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.facebook_id %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </table>
</div>
